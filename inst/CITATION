year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{ThetaProbe}: Download, Clean and Generate a Single Data File From USQ Theta Probe Soil Moisture Data",
         author = c(person("Adam", "Sparks"))
         year = year,
         note = note,
         url = "https://github.com/adamhsparks/ThetaProbe")
